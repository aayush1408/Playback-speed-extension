{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "content_scripts": [
    {
      "matches":[
        "<all_urls>" 
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action":{
    "default_icon":"icon.png",
    "default_popup":"popup.html"
  },
  "manifest_version": 2
}